model Cache {
    key       String   @id
    value     String   @db.VarChar
    ttl       Int
    expiresAt DateTime @db.Timestamptz(6)
}

model ConsoleLog {
    id        String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
    idx       Int      @default(autoincrement())
    createdAt DateTime @default(now()) @db.Timestamptz(6)

    level    String
    message  String
    request  Json?
    response Json?
    cause    Json?
}
